{namespace JIRA.Templates.ProjectTemplates.JiraCustomize}

/**
 * Render the information page for Test Project Template.
 */
{template .renderTestProjectTemplateExplanation}

    <div class="dialog-page-body template-info">
       <div class="dialog-panel-body pt-content" style="height: 241px; display: block;">
          <div>
             <div class="top-panel">
                <p>{getText('test.project.template.info.page.description')}</p>
             </div>
             <div class="left-panel">
                <h6>Issue Types</h6>
                <ul class="project-template-issuetype-list">
                   <li><span class="issuetype-list-label"><span class="issuetype-icon bug"></span><span class="issuetype-name">Test Case</span></span></li>
                   <li><span class="issuetype-list-label"><span class="issuetype-icon task"></span><span class="issuetype-name">Task</span></span></li>
               </ul>
             </div>
             <div class="right-panel">

             </div>
          </div>
       </div>
    </div>
{/template}